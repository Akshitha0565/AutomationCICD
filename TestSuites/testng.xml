<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">

    <listeners>
        <listener class-name="QA.TestComponents.Listeners"/>

        </listeners>

    <test thread-count="5" name="Submit Order Tests">
        <classes>
            <class name="QA.Tests.SubmitOrderTest"/>
        </classes>
    </test>


    <test thread-count="5" name="Error Validation Tests">
        <classes>
            <class name="QA.Tests.ErrorValidationsTests"/>
        </classes>
    </test>
</suite>